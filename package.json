{
  "name": "exdom",
  "version": "0.1.0",
  "description": "DOM extensions",
  "author": "Morris Brodersen <mb@morrisbrodersen.de>",
  "license": "ISC",
  "module": "src/index.js",
  "main": "dist/exdom.js",
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "scripts": {
    "format": "prettier --write src/**/*.js test/**/*.js *.js .babelrc package.json",
    "lint": "eslint src test",
    "test": "env NODE_ENV=test nyc mocha test",
    "build": "rollup -c",
    "validate": "npm run format && npm run lint && npm run build && npm run test"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "all": true,
    "instrument": false,
    "sourceMap": false,
    "exitOnError": true,
    "reporter": "html"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.3.1",
    "babel-plugin-istanbul": "^5.1.0",
    "body-parser": "^1.18.3",
    "eslint": "^5.12.1",
    "express": "^4.16.4",
    "jsdom": "^13.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "prettier": "^1.16.3",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-node-resolve": "^4.0.0",
    "whatwg-fetch": "^3.0.0"
  }
}
